{H:=Block:input}
  <input>
{/H:=Block:input}

{H:=Block:span}
      <span id="{H:=Block:span:id}" class="{H:=Block:span:class}">{H:=Block:span:title}</span>
{/H:=Block:span}

{H:=Block:inputText}
    <input type="text" placeholder="{H:=Block:inputText:placeholder}" id="{H:=Block:inputText:id}" class="{H:=Block:inputText:class}" name="{H:=Block:inputText:name}" value="{H:=Block:inputText:value}">
{/H:=Block:inputText}

{H:=Block:label}
    <label for="{H:=Block:inputText:for}">{H:=Block:inputText:title}</label>
{/H:=Block:label}

{H:=Block:H4}
    <h4 class="{H:=Block:h4:class}">{H:=Block:H4:title}</h4> 
{/H:=Block:H4}

{H:=Block:i}
    <i class="{H:=Block:i:class}">{H:=Block:i:icon}</i>
{/H:=Block:i}

{H:=Block:img}
  <img src="{H:=Block:img:src}" alt="{H:=Block:img:alt}" style="{H:=Block:img:style}" class="{H:=Block:img:class}">    
{/H:=Block:img}

{H:=Block:select}
    <select></select>
{/H:=Block:select} 

{H:=Block:option}
    <option></option>
{/H:=Block:option}    

{H:=Block:div}
    <div></div>
{/H:=Block:div}               